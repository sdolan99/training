<h1>Add a new existing part:</h1>
<br/>
List of parts:
<ul>
</ul>
<form action="/acquire_part/" method="post">
  Part ID: <select name='existing_part_id'>
    <% parts_list.each do |part|
         id = part['id']
         type = AcquisitionTracker::Ui::Translate.get_type(part)
         model = part["#{type}/model_number"]
    %>
      <option value=<%= id %>><%= model %></option>
    <% end %>
    </select>
    <br/>
    Acquisition Date: <input type="datetime" value="<%= Time.now %>" name="date_acquired">
    <br/>
    <input type="submit">
</form>
